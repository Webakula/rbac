{
    "name": "webakula/laravel-admin-ext-rbac",
    "description": "RBAC extension for laravel-admin",
    "type": "library",
    "keywords": ["laravel-admin", "rbac"],
    "license": "MIT",
    "authors": [
        {
            "name": "z-song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "encore/laravel-admin": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Admin\\RBAC\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Encore\\Admin\\RBAC\\RBACServiceProvider"
            ]
        }
    }
}
